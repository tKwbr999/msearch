{
  "name": "msearch",
  "version": "1.0.0",
  "description": "宮古諸島専用ハイブリッドAPI検索CLIツール - OpenStreetMap + Foursquare統合",
  "main": "dist/main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/tKwbr999/msearch.git#main",
    "directory": "."
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "homepage": "https://github.com/tKwbr999/msearch",
  "bugs": {
    "url": "https://github.com/tKwbr999/msearch/issues"
  },
  "scripts": {
    "build": "tsc && chmod +x dist/main.js",
    "start": "node dist/main.js",
    "dev": "tsc --watch",
    "test": "jest",
    "test:unit": "jest --testPathPattern=unit",
    "test:e2e": "jest --testPathPattern=e2e",
    "test:e2e:ci": "jest --testPathPattern=ci-optimized",
    "test:e2e-ci": "jest --testPathPattern=ci-optimized",
    "test:lightweight": "jest --testPathPattern=lightweight --silent",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --maxWorkers=2 --testTimeout=10000",
    "lint": "eslint src/ tests/",
    "lint:fix": "eslint src/ tests/ --fix",
    "fmt": "prettier --write \"**/*.{ts,json,md}\" --ignore-path .prettierignore",
    "fmt:check": "prettier --check \"**/*.{ts,json,md}\" --ignore-path .prettierignore"
  },
  "bin": {
    "msearch": "dist/main.js"
  },
  "files": [
    "dist/",
    "README.md",
    "DEVELOP.md",
    ".env.example"
  ],
  "keywords": [
    "cli",
    "maps",
    "search",
    "miyako",
    "okinawa",
    "japan",
    "宮古島",
    "openstreetmap",
    "overpass-api",
    "foursquare",
    "hybrid-api",
    "poi",
    "reviews",
    "ratings"
  ],
  "author": "tKwbr999",
  "license": "MIT",
  "dependencies": {
    "axios": "1.9.0",
    "dotenv": "16.5.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.0.2",
    "@eslint/js": "^9.28.0",
    "@types/node": "^22.15.30",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  }
}
