{
  "name": "msearch",
  "version": "0.4.0",
  "description": "宮古諸島専用Google Mapsコマンドライン検索ツール",
  "main": "build/miyako-maps-search.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/tKwbr999/msearch.git"
  },
  "homepage": "https://github.com/tKwbr999/msearch",
  "bugs": {
    "url": "https://github.com/tKwbr999/msearch/issues"
  },
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build",
    "start": "node build/miyako-maps-search.js",
    "dev": "ts-node miyako-maps-search.ts",
    "lint": "eslint . --ext .ts --ignore-path .eslintignore",
    "lint:fix": "eslint . --ext .ts --fix --ignore-path .eslintignore",
    "fmt": "prettier --write \"**/*.{ts,json,md}\" --ignore-path .prettierignore",
    "fmt:check": "prettier --check \"**/*.{ts,json,md}\" --ignore-path .prettierignore"
  },
  "bin": {
    "msearch": "./build/miyako-maps-search.js"
  },
  "files": [
    "build/",
    "miyako-maps-search.ts",
    "tsconfig.json",
    "README.md"
  ],
  "keywords": [
    "cli",
    "maps",
    "google-maps",
    "search",
    "miyako",
    "okinawa",
    "japan",
    "宮古島"
  ],
  "author": "tKwbr999",
  "license": "MIT",
  "dependencies": {
    "playwright": "^1.42.1",
    "cheerio": "^1.0.0-rc.12",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "@eslint/eslintrc": "^3.0.2",
    "@eslint/js": "^8.56.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2"
  }
}
